{
    "name": "redpandaventures/wp-cli-composer",
    "description": "Composer tools for use with private and commericial WordPress plugins and themes",
    "type": "wp-cli-package",
    "homepage": "https://github.com/redpandaventures/wp-cli-composer",
    "license": "MIT",
     "authors": [
        {
            "name": "Jamie Madden",
            "homepage": "https://redpandaventures.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "WP_CLI\\Composer\\": "src/"
        },
        "files": [ "command.php" ]
    },
    "require": {
        "wp-cli/wp-cli": "^2"
    },
    "require-dev": {
        "behat/behat": "~2.5"
    }
}
